{
  "name": "buildkite-mcp",
  "version": "0.2.0",
  "description": "Buildkite MCP server implementation in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "buildkite-mcp": "bin/buildkite-mcp.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "node --import ./register.js src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stdio": "node bin/buildkite-mcp.js",
    "build:start": "npm run build && npm start",
    "watch": "tsc -w",
    "prepare": "chmod +x bin/buildkite-mcp.js && npm run build"
  },
  "keywords": [
    "buildkite",
    "mcp",
    "model context protocol",
    "ai"
  ],
  "author": "Buildkite",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.1",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "@types/node-fetch": "^2.6.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  }
}
